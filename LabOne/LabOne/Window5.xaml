<Window x:Class="LabOne.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabOne"
        mc:Ignorable="d"
        Title="About" Height="300" Width="400" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="20,20,20,20">
        <TextBox x:Name="About" TextWrapping="Wrap" Background="{x:Null}" FontFamily="Times New Roman" FontSize="20" FontWeight="Bold" Margin="0,0,0,72" TextAlignment="Center"/>
        <Button Content="Main menu" Margin="90,172,90,15" FontFamily="SimSun-ExtB" FontSize="24" Click="Button_Click"/>

    </Grid>
</Window>
